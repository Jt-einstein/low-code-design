{
  "name": "root",
  "private": true,
  "devEngines": {
    "node": "8.x || 9.x || 10.x || 11.x"
  },
  "workspaces": [
    "packages/*",
    "formily/*"
  ],
  "scripts": {
    "dev": "yarn workspace low-code-playground start",
    "bootstrap": "lerna bootstrap",
    "clean": "lerna clean",
    "build": "rimraf -rf packages/*/{lib,esm} && lerna run build",
    "build:docs": "dumi build",
    "start": "dumi dev",
    "start:playground": "npm run start:basic",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "test:watch": "umi-test --watch",
    "test:prod": "umi-test --coverage --silent",
    "preversion": "npm run build && npm run lint",
    "version": "ts-node scripts/release changelog && git add -A",
    "version:alpha": "lerna version prerelease --preid alpha",
    "version:beta": "lerna version prerelease --preid beta --y",
    "version:rc": "lerna version prerelease --preid rc",
    "version:patch": "lerna version patch",
    "version:minor": "lerna version minor",
    "version:preminor": "lerna version preminor --preid beta",
    "version:major": "lerna version major",
    "release:github": "ts-node scripts/release release",
    "release:force": "lerna publish from-package --yes",
    "prelease:force": "lerna publish from-package --yes --dist-tag next",
    "pub-dev": "git pull && yarn install && yarn iconfont && lerna publish prerelease --preid dev --dist-tag dev --registry http://npm.mananacare.cn",
    "pub-prod": "git pull && yarn install && yarn iconfont && lerna publish --registry http://npm.mananacare.cn",
    "prelease": "lerna publish --dist-tag dev --registry http://npm.mananacare.cn",
    "lint": "eslint --ext .ts,.tsx,.js  --fix",
    "postinstall": "opencollective-postinstall",
    "tscheck:diff": "ts-node scripts/tsChecker",
    "tscheck:all": "lerna run tscheck",
    "iconfont": "ts-node ./scripts/iconfont",
    "modifyMonacoEditor": "node scripts/modifyMonacoEditor",
    "modifyPackages": "node scripts/modifyPackages",
    "modifyLernaGitConfig": "node scripts/modifyLernaGitConfig"
  },
  "resolutions": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "yargs": "^16.x"
  },
  "devDependencies": {
    "@alifd/next": "^1.19.1",
    "@ant-design/icons": "^4.0.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-image": "3.0.3",
    "@rollup/plugin-url": "^8.0.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/dateformat": "^3.0.1",
    "@types/fs-extra": "^8.1.0",
    "@types/hoist-non-react-statics": "^3.3.1",
    "@types/jest": "^29.1.2",
    "@types/node": "^12.6.8",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.8.2",
    "@umijs/test": "^3.5.34",
    "axios": "^1.6.8",
    "babel-jest": "^29.1.2",
    "chalk": "^2.4.2",
    "chokidar": "^2.1.2",
    "concurrently": "^4.1.0",
    "conventional-commit-types": "^2.2.0",
    "cool-path": "^1.0.6",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "dayjs": "1.11.10",
    "echarts-wordcloud": "^2.1.0",
    "escape-string-regexp": "^4.0.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-markdown": "^2.0.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "execa": "^5.0.0",
    "findup": "^0.1.5",
    "fs-extra": "^7.0.1",
    "gh-release": "^5.0.1",
    "ghooks": "^2.0.4",
    "glob": "^7.1.3",
    "identity-obj-proxy": "^3.0.0",
    "istanbul-api": "^2.1.1",
    "istanbul-lib-coverage": "^2.0.3",
    "jest": "^29.1.2",
    "jest-codemods": "^0.19.1",
    "jest-dom": "^3.1.2",
    "jest-localstorage-mock": "^2.3.0",
    "jest-styled-components": "6.3.3",
    "jest-watch-lerna-packages": "^1.1.0",
    "lerna": "^4.0.0",
    "less-plugin-npm-import": "^2.1.0",
    "lint-staged": "^8.2.1",
    "mobx": "^6.0.4",
    "mobx-react-lite": "^3.1.6",
    "onchange": "^5.2.0",
    "opencollective": "^1.0.3",
    "opencollective-postinstall": "^2.0.2",
    "param-case": "^3.0.4",
    "prettier": "^2.7.1",
    "pretty-format": "^24.0.0",
    "pretty-quick": "^3.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-mde": "^11.5.0",
    "react-speech-recognition": "3.10.0",
    "react-test-renderer": "^17.0.0",
    "request": "^2.88.2",
    "resize-observer-polyfill": "^1.5.1",
    "rimraf": "^3.0.0",
    "rollup": "^2.37.1",
    "rollup-plugin-external-globals": "^0.6.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "semver": "^7.3.5",
    "semver-regex": "^2.0.0",
    "showdown": "^1.9.1",
    "staged-git-files": "^1.1.2",
    "string-similarity": "^4.0.4",
    "styled-components": "^5.0.0",
    "ts-import-plugin": "1.6.1",
    "ts-jest": "^26.0.0",
    "ts-morph": "^19.0.0",
    "ts-node": "^9.1.1",
    "tslib": "^2.4.0",
    "typescript": "4.6.4",
    "user": "^0.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "eslint --ext .ts,.tsx,.js --fix",
      "pretty-quick --staged",
      "git add"
    ],
    "*.md": [
      "pretty-quick --staged",
      "git add"
    ]
  },
  "dependencies": {
    "antd-mobile": "^5.37.1",
    "js-audio-recorder": "^1.0.7"
  }
}
